{
  "name": "vue-form-components",
  "version": "0.1.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antonreshetov/vue-form-components.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/vfc.common.js",
  "style": "dist/vfc.css",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:docs": "APP_TARGET=docs vue-cli-service serve --mode docs",
    "build": "APP_TARGET=docs vue-cli-service build --dest docs",
    "build:bundle": "vue-cli-service build --target lib --name vfc ./src/components/index.js",
    "deploy": "build/bin/gh-deploy.sh",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "async-validator": "^1.10.0",
    "popper.js": "^1.14.4",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-plugin-unit-jest": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "@vue/eslint-config-standard": "^3.0.3",
    "@vue/test-utils": "^1.0.0-beta.20",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "highlight.js": "^9.12.0",
    "marked": "^0.5.1",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "vue-svg-loader": "^0.10.0",
    "vue-template-compiler": "^2.5.17"
  }
}
